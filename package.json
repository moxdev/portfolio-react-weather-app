{
  "name": "my-weather-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "axios": "^0.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4"
  },
  "dependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-css": "stylus -c -u autoprefixer-stylus src/static/stylus/style.styl -o src/static/css/style.css",
    "build-js": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom",
    "start": "npm-run-all -p watch-stylus start-react",
    "start-react": "react-scripts start",
    "watch-stylus": "stylus -w -u autoprefixer-stylus src/static/stylus/style.styl -o src/static/css/style.css"
  }
}
